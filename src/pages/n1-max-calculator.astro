---
import N1MaxCalculatorForm from "@/components/react/N1MaxCalculatorForm";
import BaseLayout from "@/layouts/BaseLayout.astro";
---

<BaseLayout
  title="N1 Max Calculator | Rubadue Wire"
  description="Calculate the maximum number of turns (N1 max) for a transformer using our N1 Max Calculator"
  category="Tools"
  eyebrow="Tools"
  headerTitle="N1 Max Calculator"
>
  <div class="py-24 max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <N1MaxCalculatorForm client:load />
  </div>
</BaseLayout>
