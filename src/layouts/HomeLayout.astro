---
import CoreLayout from "./CoreLayout.astro";

interface Props {
  title: string;
  description: string;
  image?: string;
}

const { title, description, image } = Astro.props;
---

<CoreLayout title={title} description={description} image={image}>
  <slot name="head" slot="head" />
  <main>
    <slot />
  </main>
</CoreLayout>
